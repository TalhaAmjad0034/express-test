{
  "cSpell.words": [
    "AISCOUT",
    "axios",
    "bgcolor",
    "Brightsign",
    "calcey",
    "cookie'ised",
    "falso",
    "hookform",
    "luxon",
    "mathjax",
    "ngneat",
    "notistack",
    "reacthookform",
    "startotype"
  ],
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "graphql"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },

  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.associations": {
    "iostream": "cpp",
    "ostream": "cpp"
  }
}
